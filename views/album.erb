<%= partial 'breadcrumbs', { :breadcrumbs => breadcrumbs(@pwd) } %>
<div id="pwd">
	<h1>Listing Contents of <%= @pwd %></h1>
</div>
<div id="songs">
	<ul>
		<% @files.each do |file| %>
			<li><a href="/<%= params[:artist] %>/<%= params[:album] %>/<%= file.split('/').last %>"><%= file.split('/').last %></a></li>
		<% end %>
	</ul>
</div>
