<%= partial 'breadcrumbs', { :breadcrumbs => breadcrumbs(@pwd) } %>
<div id="pwd">
	<h1>Listing Contents of <%= @pwd %></h1>
</div>
<div id="albums">
	<ul>
		<% @directories.each do |directory| %>
			<li><a href="/<%= params[:artist] %>/<%= File.split(directory).last %>"><%= File.split(directory).last %></a></li>
		<% end %>
	</ul>
</div>
